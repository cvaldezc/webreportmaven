<?xml version="1.0" encoding="UTF-8"?>

<Configuration>
    <Appenders>
        <File name="logComercial04" filename="logComercial04.log" append="false">
            <PatternLayout pattern="%d{HH:mm:ss} %-5p %c{2} %m%n" />
        </File>
    </Appenders>
    
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="logComercial04"/>
        </Root>
    </Loggers>
</Configuration>